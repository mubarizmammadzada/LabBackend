            </div>
        </div>
        <div class="row">
            @foreach (Blog blog in Model.Blogs)
            {
                <div class="col-md-6 col-lg-4 col-md-4">
                    <div class="single-blog">
                        <div class="thumb">
                            <img class="img-fluid" src="~/img/blog/@blog.Image" alt="">
                        </div>
                        <div class="short_details">
                            <div class="meta-top d-flex">
                                @foreach (BlogTag tag in blog.BlogTags)
                                {
                                    if (blog.BlogTags[blog.BlogTags.Count - 1] != tag)
                                    {
                                        <a href="#">@tag.Name, </a>
                                    }
                                    else
                                    {
                                        <a href="#">@tag.Name </a>
                                    }

                                }


                            </div>
                            <a class="d-block" href="single-blog.html">
                                <h4>
                                   @blog.Title
                                </h4>
                            </a>
                            <div class="meta-bottom d-flex">
                                <a href="#"><i class="ti-comments"></i>@blog.CommentCount comment</a>
                                <a href="#"><i class="ti-heart"></i> @blog.LikeCount like</a>
                            </div>
                        </div>
                    </div>
                </div>
            }

            
        </div>
    </div>
</section>


